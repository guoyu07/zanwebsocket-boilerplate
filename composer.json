{
  "name": "{{NAME}}",
  "repositories": {
    "packagist": {
      "type": "composer",
      "url": "https://packagist.phpcomposer.com"
    }
  },
  "require": {
    "zanphp/zan": "dev-master",
    "nova-service/nova-demo": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "{{NAMESPACE}}": "src",
      "{{NAMESPACE}}Test\\": "test",
      "{{NAMESPACE}}Init\\": "init"
    },
    "classmap": []
  },
  "autoload-dev": {
    "classmap": []
  },
  "require-dev": {
    "phpunit/phpunit": "4.*",
    "mockery/mockery": "~0.9.2"
  },
  "config": {
    "preferred-install": "dist",
    "secure-http": false
  },
  "minimum-stability": "dev"
}
